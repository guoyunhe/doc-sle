msgstr ""

#. Tag: literal
#: inst_yast2.xml:710
#, no-c-format
msgid "install=ftp://<replaceable>&ftpname;/PATH_TO_ISO</replaceable>"
msgstr ""

#. Tag: para
#: inst_yast2.xml:716
