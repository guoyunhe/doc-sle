msgstr ""

#. Tag: para
#: ay_bigfile.xml:1826
#, no-c-format
msgid "Specifies the strategy &ay; will use to partition the hard disk."
msgstr ""

#. Tag: para
#: ay_bigfile.xml:1830 ay_bigfile.xml:1875 ay_bigfile.xml:1909
#, no-c-format
msgid "Choose between:"
msgstr ""

#. Tag: para
#: ay_bigfile.xml:1835
#, no-c-format
msgid ""
"<literal>all</literal> (uses the whole device while calculating the new "
"partitioning),"
msgstr ""

#. Tag: para
#: ay_bigfile.xml:1841
#, no-c-format
msgid "<literal>linux</literal> (only existing Linux partitions are used),"
msgstr ""

#. Tag: para
#: ay_bigfile.xml:1847
#, no-c-format
msgid ""
"<literal>free</literal> (only unused space on the device is used, no other "
"partitions are touched),"
msgstr ""

#. Tag: para
#: ay_bigfile.xml:1853
#, no-c-format
msgid "1,2,3 (a list of comma separated partition numbers to use)."
msgstr ""

#. Tag: para
#: ay_bigfile.xml:1860
#, no-c-format
msgid "This parameter should be provided."
msgstr ""

#. Tag: literal
#: ay_bigfile.xml:1868 ay_bigfile.xml:7603
#, no-c-format
msgid "<literal>type</literal>"
msgstr ""

