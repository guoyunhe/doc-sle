#. Tag: title
#: adm_wbem.xml:738
#, no-c-format
msgid "enableHttp"
msgstr ""

#. Tag: para
#: adm_wbem.xml:740
#, no-c-format
msgid ""
"Specifies whether SFCB should accept HTTP client connections. Default is "
"<literal>false</literal> ."
msgstr ""

#. Tag: para
#: adm_wbem.xml:745
#, no-c-format
msgid "<literal>enableHttp:</literal> <replaceable>option</replaceable>"
msgstr ""

#. Tag: para
#: adm_wbem.xml:753 adm_wbem.xml:865 adm_wbem.xml:935 adm_wbem.xml:1008
#: adm_wbem.xml:1081 adm_wbem.xml:1217 adm_wbem.xml:1323
#, no-c-format
msgid "Option"
msgstr ""

#. Tag: para
#: adm_wbem.xml:758 adm_wbem.xml:870 adm_wbem.xml:940 adm_wbem.xml:1013
#: adm_wbem.xml:1086 adm_wbem.xml:1222 adm_wbem.xml:1328
