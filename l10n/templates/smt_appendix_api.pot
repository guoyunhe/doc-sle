#. Tag: para
#: smt_appendix_api.xml:217
#, no-c-format
msgid ""
"Returns a list of all software repositories known to &smt;. Those which are "
"currently mirrored on &smt; have non-empty mirror time stamp in the mirrored "
"attribute."
msgstr ""

#. Tag: screen
#: smt_appendix_api.xml:225
#, no-c-format
msgid ""
"start = element repos {\n"
"  element repo {\n"
"    attribute id {xsd:integer},        # SMT ID of the repository\n"
"    attribute name {xsd:string},       # repository's Unix name\n"
"    attribute target {xsd:string},     # repository's target product\n"
"    attribute mirrored {xsd:integer}   # time stamp of the last successful "
"mirror\n"
"                                       # (empty if not mirrored yet)\n"
"  }*\n"
"}"
msgstr ""

#. Tag: screen
#: smt_appendix_api.xml:229
#, no-c-format
msgid ""
