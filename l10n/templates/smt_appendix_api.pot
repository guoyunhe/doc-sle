#. Tag: literal
#: smt_appendix_api.xml:175
#, no-c-format
msgid "GET /repo/&lt;repoid&gt;"
msgstr ""

#. Tag: para
#: smt_appendix_api.xml:178
#, no-c-format
msgid ""
"Returns detailed information about the specified repository. The "
"<literal>&lt;repoid&gt;</literal> can be obtained using the <literal>/repos</"
"literal> or <literal>/products/&lt;productid&gt;/repos/</literal> call."
msgstr ""

#. Tag: screen
#: smt_appendix_api.xml:187
#, no-c-format
msgid ""
"start = element repo {                     # repository\n"
"  attribute id {xsd:integer},              # SMT ID of the repository\n"
"  attribute name {xsd:string},             # repository's Unix name\n"
"  attribute target {xsd:string},           # repository's target product\n"
"  attribute type {\"nu\" | \"yum\" | \"zypp\" | \"pum\"}, # type of "
"repository\n"
"  element description {xsd:string},        # description of the repository\n"
"  element localpath {xsd:string},          # path to local SMT mirror of "
"the\n"
"                                           # repository\n"
"  element url {xsd:anyURI},                # original URL of the repository\n"
"  element mirrored {\n"
"    attribute date {xsd:integer}           # timestamp of the last "
"successful\n"
"                                           # mirror (empty if not mirrored "
"yet)\n"
"  }\n"
"}"
msgstr ""

#. Tag: screen
#: smt_appendix_api.xml:191
#, no-c-format
msgid ""
"&lt;repo name=\"SLES10-SP2-Updates\" id=\"226\" target=\"sles-10-i586\" type="
"\"nu\"&gt;\n"
"  &lt;description&gt;SLES10-SP2-Updates for sles-10-i586&lt;/"
"description&gt;\n"
"  &lt;localpath&gt;/local/htdocs/repo/$RCE/SLES10-SP2-Updates/sles-10-"
"i586&lt;/localpath&gt;\n"
"  &lt;mirrored date=\"1283523440\"/&gt;\n"
"  &lt;url&gt;https://nu.novell.com/repo/$RCE/SLES10-SP2-Updates/sles-10-i586/"
"&lt;/url&gt;\n"
"&lt;/repo&gt;"
msgstr ""

