#. Tag: title
#: smt_appendix_api.xml:9
#, no-c-format
msgid "&smt; REST API"
msgstr ""

#. Tag: para
#: smt_appendix_api.xml:11
#, no-c-format
msgid ""
"The &smt; REST interface is meant for communication with &smt; clients and "
"integration into other Web services. The base URI for all the following REST "
"calls is <literal>https://YOURSMTSERVER/=/1</literal>. The &smt; server "
"responds with XML data described for each call by an RNC snippet with "
"comments."
msgstr ""

#. Tag: emphasis
#: smt_appendix_api.xml:20
#, no-c-format
msgid "Quick Reference"
msgstr ""

#. Tag: title
#: smt_appendix_api.xml:24
#, no-c-format
msgid "API for authenticating &smt; clients."
msgstr ""

#. Tag: para
#: smt_appendix_api.xml:25
#, no-c-format
msgid ""
"Used internally in the <systemitem>smt-client</systemitem> package. Not "
"intended for general administrative use!"
msgstr ""

#. Tag: screen
#: smt_appendix_api.xml:30
#, no-c-format
msgid ""
"GET /jobs                            get list of all jobs for client\n"
"GET /job/@next                       get the next job for client\n"
"GET /job/&lt;jobid&gt;                     get job with jobid for client.\n"
"                                     Note: this marks the job as retrieved\n"
"PUT /job/&lt;jobid&gt;                     update job having &lt;jobid&gt; "
"using XML data.\n"
"                                     Note: updates only retrieved jobs"
msgstr ""

#. Tag: para
#: smt_appendix_api.xml:31
#, no-c-format
msgid "For backward compatibility reasons, the following are also available:"
msgstr ""

#. Tag: screen
#: smt_appendix_api.xml:33
#, no-c-format
msgid ""
"GET /jobs/@next                      same as GET /job/@next\n"
"GET /jobs/&lt;jobid&gt;                    same as GET /job/&lt;jobid&gt;\n"
"PUT /jobs/&lt;jobid&gt;                    same as PUT /job/&lt;jobid&gt;"
msgstr ""

#. Tag: para
#: smt_appendix_api.xml:34
#, no-c-format
msgid ""
"API for general access (this needs authentication using credentials from the "
"<literal>[REST]</literal> section of <filename>smt.conf</filename>)."
msgstr ""

#. Tag: screen
#: smt_appendix_api.xml:39
#, no-c-format
msgid ""
"GET /client                          get data of all clients\n"
"GET /client/&lt;GUID&gt;                   get data of client with specified "
"GUID\n"
"GET /client/&lt;GUID&gt;/jobs              get client's job data\n"
"GET /client/&lt;GUID&gt;/patchstatus       get client's patch status\n"
"GET /client/&lt;GUID&gt;/job/@next         get client's next job\n"
"GET /client/&lt;GUID&gt;/job/&lt;jobid&gt;       get specified client job "
"data\n"
"GET /client/@all/jobs                get job data of all clients\n"
"GET /client/@all/patchstatus         get patch status of all clients\n"
"GET /repo                            get all repositories known to &smt;\n"
"GET /repo/&lt;repoid&gt;                   get details of repository with "
"&lt;repoid&gt;\n"
"GET /repo/&lt;repoid&gt;/patches           get repository's patches\n"
"GET /patch/&lt;patchid&gt;                 get patch &lt;patchid&gt; "
"details\n"
"GET /product                         get list of all products known to "
"&smt;\n"
"GET /product/&lt;productid&gt;             get details of product with &lt;"
"productid&gt;\n"
"GET /product/&lt;productid&gt;/repos       get list of product's repositories"
msgstr ""

#. Tag: para
#: smt_appendix_api.xml:41
#, no-c-format
msgid ""
"For backward compatibility reasons, plural forms are also available; for "
"example:"
msgstr ""

#. Tag: screen
#: smt_appendix_api.xml:45
#, no-c-format
msgid ""
"GET /clients                         same as GET /client\n"
"GET /repos                           same as GET /repo\n"
"GET /product                         same as GET /product"
msgstr ""

