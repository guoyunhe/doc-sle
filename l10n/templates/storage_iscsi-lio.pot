msgid "Log in to the host server as the &rootuser; user."
msgstr ""

#. Tag: para
#: storage_iscsi-lio.xml:852
#, no-c-format
msgid "At a terminal console prompt, enter the following commands:"
msgstr ""

#. Tag: screen
#: storage_iscsi-lio.xml:855
#, no-c-format
msgid ""
"mkdir -p /var/lib/xen/images/vm_one\n"
"dd if=/dev/zero of=/var/lib/xen/images/vm_one/xen-0 seek=1M bs=4096 count=1"
msgstr ""

