#. Tag: title
#: image_kiwi.xml:506
#, no-c-format
msgid "&kiwi; Configuration File"
msgstr ""

#. Tag: screen
#: image_kiwi.xml:507
#, no-c-format
msgid ""
"&lt;image schemeversion=\"2.0\" name=\"...\"&gt; <co xml:id=\"co.kiwi.config."
"image\"/>\n"
"  &lt;description type=\"system\"&gt; <co xml:id=\"co.kiwi.config.description"
"\"/>\n"
"    &lt;author&gt;...&lt;/author&gt;\n"
"    &lt;contact&gt;...&lt;/contact&gt;\n"
"    &lt;specification&gt;...&lt;/specification&gt;\n"
"  &lt;/description&gt;<!--\n"
"  &lt;profiles> <co id=\"co.kiwi.config.profiles\"/>\n"
"    &lt;profile name=\"...\" description=\"...\"/>\n"
"    &lt;&#x21;&#x2d;&#x2d; List of profiles reduced &#x2d;&#x2d;&gt;\n"
"  &lt;/profiles>-->\n"
"  &lt;preferences&gt; <co xml:id=\"co.kiwi.config.preferences\"/>\n"
"    &lt;type primary=\"true\" boot=\"...\" flags=\"...\"&gt;iso&lt;/"
"type&gt;\n"
"    &lt;type boot=\"...\" filesystem=\"ext3\" format=\"vmdk\"&gt;vmx&lt;/"
"type&gt;\n"
"    &lt;type boot=\"...\" filesystem=\"ext3\"&gt;xen&lt;/type&gt;\n"
"    &lt;type boot=\"...\" filesystem=\"squashfs\" flags=\"unified\"&gt;"
"oem&lt;/type&gt;\n"
"    &lt;version&gt;2.7.0&lt;/version&gt;\n"
"    &lt;size unit=\"M\"&gt;780&lt;/size&gt;\n"
"    &lt;packagemanager&gt;zypper&lt;/packagemanager&gt;\n"
"    &lt;rpm-check-signatures&gt;False&lt;/rpm-check-signatures&gt;\n"
"    &lt;rpm-force&gt;False&lt;/rpm-force&gt;\n"
"    &lt;locale&gt;en_US.UTF-8&lt;/locale&gt;\n"
"    &lt;oem-swap&gt;no&lt;/oem-swap&gt;\n"
"    &lt;oem-boot-title&gt;USB&lt;/oem-boot-title&gt;\n"
"  &lt;/preferences&gt;\n"
"  &lt;users group=\"users\"&gt; <co xml:id=\"co.kiwi.config.users\"/>\n"
"    &lt;user name=\"root\" pwd=\"\" home=\"/root\"/&gt;\n"
"  &lt;/users&gt;\n"
"  &lt;repository type=\"rpm-md\"&gt; <co xml:id=\"co.kiwi.config.repository"
"\"/>\n"
"    &lt;source path=\"/home/rpmdir\"/&gt;\n"
"  &lt;/repository&gt;\n"
"  &lt;packages type=\"image\" patternPackageType=\"onlyRequired\"&gt; <co "
"xml:id=\"co.kiwi.config.packages\"/>\n"
"    &lt;package name=\"yast2-live-installer\"/&gt;\n"
"    &lt;package name=\"pam\"/&gt;\n"
"    &lt;!-- List of packages reduced --&gt;\n"
"  &lt;/packages&gt;<!--\n"
"  &lt;split> <co id=\"co.kiwi.config.\"/>\n"
"    &lt;temporary>\n"
"      &lt;file name=\"/var\"/>\n"
"      &lt;file name=\"/var/*\"/>\n"
"      &lt;file name=\"/etc\"/>\n"
"      &lt;file name=\"/etc/*\"/>\n"
"    &lt;/temporary>\n"
"  &lt;/split>-->"
msgstr ""

