#. Tag: title
#: smt_scripts.xml:1514
#, no-c-format
msgid "&smt; &systemd; Commands"
msgstr ""

#. Tag: para
#: smt_scripts.xml:1515
#, no-c-format
msgid ""
"You can operate &smt; related services with the standard &systemd; commands:"
msgstr ""

#. Tag: command
#: smt_scripts.xml:1521
#, no-c-format
msgid "systemctl start smt.target"
msgstr ""

#. Tag: para
#: smt_scripts.xml:1524
#, no-c-format
msgid "Start the &smt; services."
msgstr ""

#. Tag: command
#: smt_scripts.xml:1530
#, no-c-format
msgid "systemctl stop smt.target"
msgstr ""

#. Tag: para
#: smt_scripts.xml:1533
#, no-c-format
msgid "Stop the &smt; services."
msgstr ""

#. Tag: command
#: smt_scripts.xml:1539
#, no-c-format
msgid "systemctl status smt.target"
msgstr ""

#. Tag: para
#: smt_scripts.xml:1542
#, no-c-format
msgid ""
"Check the status of the &smt; services. Checks whether httpd, &mariadb;, and "
"cron are running."
msgstr ""

#. Tag: command
#: smt_scripts.xml:1549
#, no-c-format
msgid "systemctl restart smt.target"
msgstr ""

#. Tag: para
#: smt_scripts.xml:1552
#, no-c-format
msgid "Restart the &smt; services."
msgstr ""

#. Tag: command
#: smt_scripts.xml:1558
#, no-c-format
msgid "systemctl try-restart smt.target"
msgstr ""

#. Tag: para
#: smt_scripts.xml:1561
#, no-c-format
msgid "Check whether the &smt; services are enabled and if so, restart them."
msgstr ""

#. Tag: para
#: smt_scripts.xml:1567
#, no-c-format
msgid "You can enable and disable &smt; with the &yast; &smt; Server module."
msgstr ""
