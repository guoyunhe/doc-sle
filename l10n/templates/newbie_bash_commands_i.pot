#, no-c-format
msgid "<option>-f</option>"
msgstr ""

#. Tag: para
#: newbie_bash_commands_i.xml:469
#, no-c-format
msgid ""
"Writes the output to a file and not to the screen as is usually the case"
msgstr ""

#. Tag: option
#: newbie_bash_commands_i.xml:476
#, no-c-format
msgid "<option>-c</option>"
msgstr ""

#. Tag: para
#: newbie_bash_commands_i.xml:479
#, no-c-format
msgid "Creates a new tar archive"
msgstr ""

#. Tag: para
#: newbie_bash_commands_i.xml:488
#, no-c-format
msgid "Adds files to an existing archive"
msgstr ""

