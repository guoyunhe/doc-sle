#, no-c-format
msgid "<secondary>locate</secondary>"
msgstr ""

#. Tag: primary
#: newbie_bash_commands_i.xml:570
#, no-c-format
msgid "<primary>locate</primary>"
msgstr ""

#. Tag: secondary
