#. Tag: para
#: suse_kb.xml:26
#, no-c-format
msgid ""
"To standardize the keyboard mapping of programs, changes were made to the "
"following files:"
msgstr ""

#. Tag: screen
#: suse_kb.xml:31
#, no-c-format
msgid ""
"/etc/inputrc\n"
"/etc/X11/Xmodmap\n"
"/etc/skel/.emacs\n"
"/etc/skel/.gnu-emacs\n"
"/etc/skel/.vimrc\n"
"/etc/csh.cshrc\n"
"/etc/termcap\n"
"/usr/share/terminfo/x/xterm\n"
"/usr/share/X11/app-defaults/XTerm\n"
"/usr/share/emacs/<replaceable>VERSION</replaceable>/site-lisp/term/*.el"
msgstr ""

